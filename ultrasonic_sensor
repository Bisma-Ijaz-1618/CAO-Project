int trig=4;
int echo=23;
int buzz=2;
void setup(){
  Serial.begin(9600);
  pinMode(trig,OUTPUT);
  pinMode(buzz,OUTPUT);
  pinMode(echo,INPUT);
}
void loop(){
  digitalWrite(trig,HIGH);
  delayMicroseconds(10);
  digitalWrite(trig,LOW);
  long duration = pulseIn(echo, HIGH);
  int distance = (duration * 0.034)/2;
  if(distance<=10){
    digitalWrite(buzz,HIGH);
  }
  else{
    digitalWrite(buzz,LOW);
  }
  Serial.println(distance);
  }
